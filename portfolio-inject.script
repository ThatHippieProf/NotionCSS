<script>
  window.onload = function() {
    const copyright = document.createElement('p')
    copyright.innerHTML = 'Â© That Hippie Prof 2021. All Rights Reserved.'
    copyright.style.color = '#000'
    copyright.style.margin = '25px 0 0 0'

    const img = document.createElement("img")
    img.setAttribute('src', 'https://thathippieprof.square.site/uploads/b/a5f4f76b89c5ddb2b9626cb74c3fead9456c7738787d415ab059136f74d5c6a4/THP_circles_1616042086.png?width=400')
